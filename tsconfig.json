{
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "Node16",
    "target": "ES2022",

    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node16",
    "outDir": "dist",

    "strict": true
  }
}
